{
  "name": "pizzalover",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-scripts": "1.1.0",
    "react-router-redux": "^4.0.8",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-hot-loader": "^3.1.2",
    "reactstrap": "^5.0.0-alpha.3",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-immutable": "^4.0.0",
    "immutable": "^3.8.2",
    "react-router-dom": "^4.2.2",
    "bootstrap": "^4.0.0-beta.2"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && NODE_ENV=production webpack --progress --bail --env dist -p",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "lint-sass": "sass-lint -c .sass-lint.yml 'src/styles/**/*.scss' -v -q",
    "reinstall": "rimraf node_modules && yarn",
    "serve:dev": "webpack-dev-server --open --env dev",
    "serve:dist": "webpack-dev-server NODE_ENV=production --open --env dist -p --progress",
    "start": "npm run serve:dev",
    "test": "cross-env NODE_PATH=src NODE_ENV=test ava --concurrency 18",
    "test:watch": "npm run test -- --watch",
    "test:debug": "cross-env NODE_PATH=src NODE_ENV=test inspect node_modules/ava/profile.js",
    "test:clean": "rimraf ./coverage ./.nyc_output",
    "test:coverage": "nyc npm run test",
    "test:coverage:verify": "nyc check-coverage",
    "test:coverage:html": "nyc report --reporter=html",
    "test:coverage:xml": "nyc report --reporter=cobertura",
    "test:coverage:html:open": "opn coverage/index.html",
    "test:coverage:report": "run-s test:coverage:html test:coverage:html:open",
    "translate": "jsonsort src/translations",
    "review": "run-s lint lint-sass test:clean test:coverage test:coverage:verify test:coverage:report"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill",
      "./config/testing/ava_setup.js"
    ],
    "babel": {
      "babelrc": true
    }
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.1",
    "json-sort-cli": "^1.0.5",
    "css-loader": "^0.28.7",
    "dotenv-webpack": "^1.5.4",
    "enzyme": "^2.8.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "jsdom": "^11.3.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "expect": "^1.20.2",
    "expect-enzyme": "^0.14.1",
    "expect-jsx": "^5.0.0",
    "mock-require": "^2.0.2",
    "node-sass": "^4.6.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.3.0",
    "postcss-loader": "^2.0.8",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "engines": {
    "node": ">=8.0.0",
    "yarn": ">=1.0.0"
  }
}